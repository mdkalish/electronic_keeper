$('#daily-turnover').html('<%= daily_turnover %>');
$('#underway-tickets').html('<%= daily_tickets("underway") %>');
$('#open-tickets').html('<%= daily_tickets("open") %>');
$('#closed-tickets').html('<%= daily_tickets("closed") %>');
<% if params[:status] != "closed" %>
  $('#delivery-button').css('visibility', 'visible').find('input').attr('value', '<%= @ticket.translate_delivery %>');
  $('#current-ticket-summary').html('<%= current_ticket_summary(@ticket) %>');
  $('#current-ticket').html('<%= j render("tickets/show_ticket") %>');
  $('#current-tickets').html('<%= j render("tickets/current_tickets") %>').find('#<%= @ticket.id %>').remove();
<% end %>
